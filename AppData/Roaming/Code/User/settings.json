{
    "git.autofetch": true,
    "remote.SSH.remotePlatform": {
        "tdt4260-sigurdht.idi.ntnu.no": "linux",
        "aurora.iet.ntnu.no": "linux",
        "venus.tele.ntnu.no": "linux",
        "odroid": "linux"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "editor.bracketPairColorization.enabled": true,
    "files.associations": {
        "*.v": "systemverilog",
        "*.gfv": "systemverilog",
        "*.tda": "systemverilog",
        "*.env": "shellscript",
        ".local.shellrc": "shellscript",
        ".shellrc": "shellscript",
        "*.c": "c",
        "*.h": "c",
        "*.s": "arm",
        "*.su": "plaintext",
        ".zpreztorc": "shellscript"
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "editor.unicodeHighlight.invisibleCharacters": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.closingBraceHints.enable": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "latex-workshop.latex.autoBuild.run": "onSave",
    "latex-workshop.docker.image.latex": "blang/latex",
    "diffEditor.ignoreTrimWhitespace": false,
    "markdown-pdf.margin.top": "1in",
    "markdown-pdf.margin.right": "1in",
    "markdown-pdf.margin.left": "1in",
    "markdown-pdf.margin.bottom": "1in",
    "markdown-pdf.scale": 0.8,
    "git.suggestSmartCommit": false,
    "notebook.outputLineHeight": 1.2,
    "workbench.editor.enablePreview": false,
    "workbench.editorAssociations": {
        "*.svg": "svgPreviewer.customEditor"
    },
    "svg.preview.transparencyGrid": false,
    "svg.preview.boundingBox": false,
    "cSpell.language": "en,no,de",
    // "cSpell.enabled": false,
    "cSpell.enabledLanguageIds": [
        "latex"
    ],
    "[python]": {
        "editor.formatOnType": true
    },
    "editor.renderWhitespace": "none",
    "cmake.configureOnOpen": true,
    "cSpell.userWords": [
        "clearify",
        "datasheet",
        "microcontroller",
        "perfectioning"
    ],
    "files.exclude": {
        "**/.trunk/*out/": true,
        "**/.trunk/*actions/": true,
        "**/.trunk/*logs/": true,
        "**/.trunk/*plugins/": true
    },
    "trunk.trunkGrayOutNonBlockingIssues": false,
    "files.watcherExclude": {
        "**/.trunk/*out/": true,
        "**/.trunk/*actions/": true,
        "**/.trunk/*logs/": true,
        "**/.trunk/*plugins/": true
    },
    "editor.defaultFormatter": "ms-python.black-formatter",
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "[c]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "gitlens.currentLine.enabled": false,
    "window.newWindowDimensions": "maximized",
    "workbench.startupEditor": "none",
    "window.title": "${rootName}${separator}${dirty}${activeEditorShort}${separator}${profileName}",
    "window.titleSeparator": " – ",
    "terminal.integrated.tabs.separator": " – ",
    "latex-workshop.latex.rootFile.indicator": "\\begin{document}",
    "latex-workshop.view.pdf.internal.synctex.keybinding": "double-click",
    "workbench.list.openMode": "doubleClick",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.wordBasedSuggestions": false,
    "latex-workshop.texcount.autorun": "onSave",
    "[latex]": {
        "editor.defaultFormatter": "James-Yu.latex-workshop",
        "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_"
    },
    "editor.formatOnSave": true,
    "workbench.tree.expandMode": "doubleClick",
    "workbench.colorTheme": "Monokai",
    // Enable quick suggestions in BibTeX
    "[bibtex]": {
        "editor.quickSuggestions": {
            "comments": true,
        },
        "editor.defaultFormatter": "James-Yu.latex-workshop"
    },
    "latex-workshop.bibtex-format.tab": "4 spaces",
    "latex-workshop.bibtex-format.trailingComma": true,
    // "cortex-debug.stlinkPath": "/home/sigurd/st/stm32cubeide_1.11.2/plugins/com.st.stm32cube.ide.mcu.externaltools.stlink-gdb-server.linux64_2.0.400.202209281104/tools/bin/ST-LINK_gdbserver",
    // "cortex-debug.armToolchainPath": "",
    // "cortex-debug.gdbPath": "/home/sigurd/downloads/gcc-arm-none-eabi-10.3-2021.10/bin/arm-none-eabi-gdb",
    // "cortex-debug.gdbPath": "/home/sigurd/st/stm32cubeide_1.11.2/plugins/com.st.stm32cube.ide.mcu.externaltools.gnu-tools-for-stm32.10.3-2021.10.linux64_1.0.100.202210260954/tools/bin/arm-none-eabi-gdb",
    "cortex-debug.variableUseNaturalFormat": true,
    "C_Cpp.autoAddFileAssociations": false,
    "editor.fontFamily": "'Fira Code', 'CodeNewRoman Nerd Font Mono', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "[plaintext]": {
        // "editor.unicodeHighlight.ambiguousCharacters": false,
        // "editor.unicodeHighlight.invisibleCharacters": false,
        "editor.fontLigatures": false
    },
    "[vhdl]": {
        "editor.fontLigatures": false
    },
    "editor.fontWeight": "400",
    "scm.defaultViewMode": "tree",
    "search.defaultViewMode": "tree",
    "editor.fontSize": 13,
    "rust-analyzer.lens.implementations.enable": false,
    "workbench.activityBar.visible": false,
    "rust-analyzer.lens.run.enable": false,
    "rust-analyzer.lens.debug.enable": false,
    "explorer.sortOrder": "mixed",
}
