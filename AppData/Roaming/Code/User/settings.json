{
    "git.autofetch": true,
    "remote.SSH.remotePlatform": {
        "tdt4260-sigurdht.idi.ntnu.no": "linux",
        "aurora.iet.ntnu.no": "linux",
        "venus.tele.ntnu.no": "linux",
        "odroid": "linux"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "editor.bracketPairColorization.enabled": true,
    "workbench.colorTheme": "Monokai",
    "files.associations": {
        "*.v": "systemverilog",
        "*.gfv": "systemverilog",
        "*.tda": "systemverilog"
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "editor.unicodeHighlight.invisibleCharacters": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.closingBraceHints.enable": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "latex-workshop.latex.autoBuild.run": "onSave",
    "latex-workshop.docker.image.latex": "blang/latex",
    "latex-workshop.synctex.afterBuild.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "markdown-pdf.margin.top": "1in",
    "markdown-pdf.margin.right": "1in",
    "markdown-pdf.margin.left": "1in",
    "markdown-pdf.margin.bottom": "1in",
    "markdown-pdf.scale": 0.8,
    "git.suggestSmartCommit": false,
    "notebook.outputLineHeight": 1.2,
    "workbench.editor.enablePreview": false,
    "explorer.openEditors.visible": 0,
    "workbench.editorAssociations": {
        "*.svg": "svgPreviewer.customEditor"
    },
    "svg.preview.transparencyGrid": false,
    "svg.preview.boundingBox": false,
    "cSpell.language": "en,no,de",
    // "cSpell.enabled": false,
    "cSpell.enabledLanguageIds": [
        "latex"
    ],
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",
}
